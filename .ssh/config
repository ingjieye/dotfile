SetEnv TERM=xterm-256color

Include ~/.ssh/work_hosts
Include ~/.ssh/jenkins_hosts  
Include ~/.ssh/home_hosts

Host github.com
  HostName ssh.github.com
  Port 443
  User git

# 全局默认配置
Host *
  ServerAliveInterval 180
  HashKnownHosts no
  #AddressFamily inet
  Compression yes
  ControlMaster auto
  ControlPath /tmp/ssh_%r@%h:%p
  ControlPersist 2h
  ForwardAgent yes
  SendEnv LANG LC_ALL
