SetEnv TERM=xterm

Host *
  ServerAliveInterval 180
  HashKnownHosts no
  #AddressFamily inet
  Compression yes
  ControlMaster auto
  ControlPath /tmp/ssh_%r@%h:%p
  ControlPersist 2h
  ForwardAgent yes
  SendEnv LANG LC_ALL

Host github.com                                                                                                          
    Hostname ssh.github.com                                                                                          
    Port 443                                                                                                         
    User git
